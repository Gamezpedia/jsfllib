fl.outputPanel.clear();fl.runScript(fl.configURI+"Javascript/FrameInfo.jsfl")//fl.trace(convertMergeXML(TimelineUtil.getSelectedFrameInfo(null,true)));/*applyXML(fl.getDocumentDOM().getTimeline(),<timeline>  <layer index="0" name="layer1">    <frame index="0" name="_up">      <actionScript/>    </frame>    <frame index="1" name="_down">      <actionScript/>    </frame>    <frame index="2" name="_enable">      <actionScript/>    </frame>    <frame index="3" name="_disable">      <actionScript/>    </frame>  </layer>  <layer index="1" name="layer2">    <frame index="0" name="">      <actionScript>stop();</actionScript>    </frame>    <frame index="2" name="">      <actionScript>dispatchEvent(new Event(Event.COMPLETE));</actionScript>    </frame>  </layer></timeline>);*///FrameInfo配列からマージしたXMLを得るfunction convertMergeXML(frameInfos){	var timeline = <timeline></timeline>	for each(var info in frameInfos){		var target= info.convertXML();		var layer = timeline.layer.(@index == target.layer.@index)[0]||					timeline.appendChild(<layer index={target.layer.@index} name={target.layer.@name} />).layer.(@index == target.layer.@index)[0];		layer.appendChild(target.layer.frame)	}	return timeline}function applyXML(timeline,xml){	//フレーム生成の基準点を設定	var layerOrigin = timeline.getSelectedFrames()[0] || 0;	var frameOrigin = timeline.getSelectedFrames()[1] || 0;		for each(var layerNode in xml.layer){		var layerIndex = Number(layerNode.@index)+layerOrigin;		var layer = timeline.layers[layerIndex];		if(!layer){			timeline.addNewLayer(layerNode.@name,"normal",false)			layer = timeline.layers[layerIndex];		}		timeline.setSelectedLayers(layerIndex);		for each(var frameNode in layerNode.frame){			var frameIndex = Number(frameNode.@index)+frameOrigin;			var frame = layer.frames[frameIndex];			if(!frame || frame.startFrame != frameIndex){				//convertToKeyframesはfram配列とマッチしていない				timeline.convertToKeyframes(frameIndex);				frame = layer.frames[frameIndex];			}			frame.name = frameNode.@name;			frame.actionScript = frameNode.actionScript.text();		}	}	timeline.setSelectedLayers(layerOrigin);}